{
  "name": "leuchtfeuer/mautic-auth0-bundle",
  "description": "Auth0 is the solution you need for web, mobile, IoT, and internal applications. Loved by developers and trusted by enterprises.",
  "keywords": [
    "Mautic",
    "Auth0",
    "SSO",
    "Login",
    "User"
  ],
  "type": "mautic-plugin",
  "license": "GPL-3.0-or-later",
  "homepage": "https://www.Leuchtfeuer.com",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Leuchtfeuer Digital Marketing GmbH",
      "email": "mautic-plugins@Leuchtfeuer.com",
      "homepage": "https://www.Leuchtfeuer.com"
    }
  ],
  "support": {
    "issues": "https://github.com/Leuchtfeuer/auth0-for-mautic/issues",
    "source": "https://github.com/Leuchtfeuer/auth0-for-mautic",
    "email": "mautic-plugins@Leuchtfeuer.com"
  },
  "autoload": {
    "psr-4": {
      "MauticPlugin\\LeuchtfeuerAuth0Bundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MauticPlugin\\LeuchtfeuerAuth0Bundle\\Tests\\": "Tests"
    }
  },
  "extra": {
    "install-directory-name": "LeuchtfeuerAuth0Bundle"
  },
  "replace": {
    "bitmotion/mautic-auth0-bundle": "self.version"
  },
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.2",
    "mautic/core-lib": "^5.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "fixcs": "./vendor/bin/php-cs-fixer fix"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mautic/FOSOAuthServerBundle.git"
    },
    {
      "type": "git",
      "url": "https://github.com/mautic/SpBundle.git"
    },
    {
      "type": "git",
      "url": "https://github.com/mautic/SymfonyBridgeBundle.git"
    }
  ]
}
